<a target="_blank" rel="nofollow" href="{{ url }}" onmousedown='
  CC.track("{{ ts.event('AD_CLICK') }}", {
    ad: {
      provider: "Canvas Network",
      unit: "Banner Ads",
      title: "{{ desc }}"
    }
  });ga("send","event", "Banner Ad Click", "{{ desc }}","{{ location }}");'>
      <img src="{{ imageUrl }}" style="max-width: 728px; width: 100%" />
</a>
