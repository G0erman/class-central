{% extends 'ClassCentralSiteBundle::base.html.twig' %}

{% block headmeta %}
    <title>MOOCWatch - Understand what’s going on in the World of MOOCs</title>
    <meta name="description"
          content="Understand what’s going on in the World of MOOCs with MOOCWatch. Receive a monthly digest of everything related to MOOCs"/>
    <meta property="og:title" content="MOOCWatch - Understand what’s going on in the World of MOOCs" />
    <meta property="og:url" content="{{ baseurl }}{{ path('moocwatch') }}" />
    <meta property="og:image" content="https://www.class-central.com/bundles/classcentralsite/slashpixel/images/ccprofilepic.png">
    <meta property="og:site_name" content="Class Central"/>
    <meta property="og:description" content="Understand what’s going on in the World of MOOCs with MOOCWatch. Receive a monthly digest of everything related to MOOCs" />
    <meta property="og:type" content="website" />

    <!-- Twitter card integration -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@classcentral">
    <meta name="twitter:title" content="MOOCWatch - Understand what’s going on in the World of MOOCs">
    <meta name="twitter:description" content="Receive a monthly digest of everything related to MOOCs">
    <meta name="twitter:domain" content="www.class-central.com">
{% endblock %}




{% block contentheader %}
    <div class="mooc-watch-banner">
        <div class="container mooc-watch-banner-content">
            <h1 class="heading-two-colors mooc-watch-title"><span class="heading-color-first">Mooc</span><span class="heading-color-second">watch</span></h1>
            <div class="heading-tagline">Understand what’s going on in the <span class="heading-tagline__highlight">World of MOOCs</span></div>
            <form action="//class-central.us8.list-manage.com/subscribe/post?u=a04149ada492173efe6609e68&amp;id=c5a9919ea5" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form"  class="mooc-digest-subscription inline-form validate"  novalidate>
                <span class="mooc-digest-subscription__title">Receive a monthly digest of everything related to MOOCs</span>
                <p class="cc-inline-form-group">

                    <input class="cc-inline-form-element cc-inline-form-element--input" type="email" placeholder="Your email address" name="EMAIL" required id="mce-EMAIL">
                    <input class="cc-inline-form-element cc-inline-form-element--button" type="submit" value="Subscribe to MOOCWatch">
                </p>
                <div style="position: absolute; left: -5000px;"><input type="text" name="b_a04149ada492173efe6609e68_c5a9919ea5" tabindex="-1" value=""></div>
                <span class="mooc-digest-subscription__smallprint">We hate spam - unsubscribe any time.</span>
            </form>
        </div>
    </div>

{% endblock %}



{% block abovefooter %}

    <section id="mooc-watch-view-section" class="mooc-watch-view-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mooc-watch-title-col">
                    <h2 class="mooc-watch-view-section__title">October 2014 edition</h2>
                    <span class="mooc-watch-view-section__tagline">Take a peek at last month’s MOOCWatch</span>
                    <a href="https://docsend.com/view/6jdri9t" class="cc-button mooc-watch-view-section__button" target="_blank">View October’s MOOCWatch</a>
                </div>
                <div class="col-lg-4 mooc-info-col">
                    <div class="paper-stack-wrap">

                        <div class="paper-stack">

                            <div class="paper-stack__top-sheet">
                                <div class="mooc-watch-info">

                                    <h2 class="mooc-watch-info__title">
                                        <svg class="icon icon-cc-logo-circle" viewBox="0 0 32 32">
                                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-cc-logo-circle"></use>
                                        </svg>
                                        <span class="mooc-watch-info__title-colored">MOOC</span>WATCH
                                    </h2>

                                    <div class="mooc-watch-info__item-group">
                                        <div class="mooc-watch-info__item">
                                            <div class="mooc-watch-info__item-content">
                                                <span class="mooc-watch-info__item-count">253</span>
                                                <span class="mooc-watch-info__item-key">Courses<br>started</span>
                                            </div>
                                        </div>
                                        <div class="mooc-watch-info__item">
                                            <div class="mooc-watch-info__item-content">
                                                <span class="mooc-watch-info__item-count">142</span>
                                                <span class="mooc-watch-info__item-key">Courses<br>Added</span>
                                            </div>
                                        </div>
                                        <div class="mooc-watch-info__item">
                                            <div class="mooc-watch-info__item-content">
                                                <span class="mooc-watch-info__item-count">2224</span>
                                                <span class="mooc-watch-info__item-key">Total<br>MOOCs</span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="mooc-watch-info__news">
                                        <h3 class="mooc-watch-info__news-title">Top Stories</h3>
                                        <div class="mooc-watch-info__news-item">
                                            <a href="https://www.edx.org/blog/we-are-launching-high-school-initiative" target="_blank" class="mooc-watch-info__news-item-title">edX to offer free AP courses</a>
                                            <span class="mooc-watch-info__news-item-provider">edX Blog</span>
                                        </div>
                                        <div class="mooc-watch-info__news-item">
                                            <a href="" class="mooc-watch-info__news-item-title" target="_blank">Udacity Raises $35 million</a>
                                            <span class="mooc-watch-info__news-item-provider">Udacity Blog</span>
                                        </div>
                                        <div class="mooc-watch-info__news-item">
                                            <a href="https://www.class-central.com/report/caltech-mooc-flipped-classroom/" target="_blank" class="mooc-watch-info__news-item-title">Parallel Sessions: MOOC Meets Flipped Classrom</a>
                                            <span class="mooc-watch-info__news-item-provider">MOOC Report</span>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                    </div>


                </div>

            </div>

        </div>

    </section>

    <section id="mooc-watch-testimonials-section" class="mooc-watch-testimonials-section">

        <div class="container">

            <h2 class="mooc-watch-testimonials-section__title">What the experts say...</h2>

            <div class="testimonial-card mooc-watch-testimonials-section__testimonial-card" >

                <div class="testimonial-card__author-block">
                    <div class="testimonial-card__author-content">
                        <div class="crop-circle crop-circle--bordered">
                            <img src="/bundles/classcentralsite/slashpixel/images/lorenabarba.png" alt="Lorena Barba">
                        </div>
                        <span class="testimonial-card__title">Lorena A. Barba</span>
                        <span class="testimonial-card__meta">Associate Professor, George Washington University</span>
                    </div>
                </div>

                <div class="testimonial-card__testimonial-block">
                    <p class="testimonial-card__testimonial">
                        If you search for the term MOOC on Google today, you'll get more than 8 million hits. How do you know what matters? Class Central's MOOCWatch is a time-saving digest of
                        the top figures and events, compiled with the know-how of long-time MOOC watchers and takers.
                    </p>
                </div>
            </div>

            <div class="testimonial-card mooc-watch-testimonials-section__testimonial-card">

                <div class="testimonial-card__testimonial-block testimonial-card__testimonial-block--left">
