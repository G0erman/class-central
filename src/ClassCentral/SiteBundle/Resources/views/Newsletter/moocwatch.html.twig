{% extends 'ClassCentralSiteBundle::base.html.twig' %}

{% block headmeta %}
    <title>Newsletter subscription confirmed</title>
    <meta name="description"
          content="Newsletter subscription confirmed"/>
    <meta property="og:title" content="Newsletter subscription confirmed" />
    <meta property="og:url" content="{{ baseurl }}{{ path('newsletter_subscribed') }}" />
    <meta property="og:image" content="https://www.class-central.com/bundles/classcentralsite/slashpixel/images/ccprofilepic.png">
    <meta property="og:site_name" content="Class Central"/>
    <meta property="og:description" content="Newsletter subscription confirmed" />
    <meta property="og:type" content="website" />
    <meta property="fb:admins" content="{{ fb_admins }}" />

    <!-- Twitter card integration -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@classcentral">
    <meta name="twitter:title" content="Newsletter subscription confirmed">
    <meta name="twitter:description" content="Newsletter subscription confirmed">
    <meta name="twitter:domain" content="www.class-central.com">
{% endblock %}




{% block contentheader %}
    <div class="mooc-watch-banner">
        <div class="container mooc-watch-banner-content">
            <h1 class="heading-two-colors mooc-watch-title"><span class="heading-color-first">Mooc</span><span class="heading-color-second">watch</span></h1>
            <div class="heading-tagline">Understand what’s going on in the <span class="heading-tagline__highlight">World of MOOCs</span></div>
            <form class="mooc-digest-subscription inline-form">
                <span class="mooc-digest-subscription__title">Receive a monthly digest of everything related to MOOCs</span>
                <p class="cc-inline-form-group">
                    <input class="cc-inline-form-element cc-inline-form-element--input" type="email" placeholder="Your email address" name="email" required="">
                    <input class="cc-inline-form-element cc-inline-form-element--button" type="submit" value="Subscribe to MOOC Watch">
                </p>
                <span class="mooc-digest-subscription__smallprint">We hate spam - unsubscribe any time.</span>
            </form>
        </div>
    </div>

{% endblock %}



{% block abovefooter %}

    <section id="mooc-watch-view-section" class="mooc-watch-view-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mooc-watch-title-col">
                    <h2 class="mooc-watch-view-section__title">September 2014 edition</h2>
                    <span class="mooc-watch-view-section__tagline">Take a peek at last month’s MOOC Watch</span>
                    <a href="" class="cc-button mooc-watch-view-section__button">View September’s MOOC Watch</a>
                </div>
                <div class="col-lg-4 mooc-info-col">
                    <div class="paper-stack-wrap">

                        <div class="paper-stack">

                            <div class="paper-stack__top-sheet">
                                <div class="mooc-watch-info">

                                    <h2 class="mooc-watch-info__title">
                                        <svg class="icon icon-cc-logo-circle" viewBox="0 0 32 32">
                                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#icon-cc-logo-circle"></use>
                                        </svg>
                                        <span class="mooc-watch-info__title-colored">MOOC</span>WATCH
                                    </h2>

                                    <div class="mooc-watch-info__item-group">
                                        <div class="mooc-watch-info__item">
                                            <div class="mooc-watch-info__item-content">
                                                <span class="mooc-watch-info__item-count">328</span>
                                                <span class="mooc-watch-info__item-key">Courses<br>started</span>
                                            </div>
                                        </div>
                                        <div class="mooc-watch-info__item">
                                            <div class="mooc-watch-info__item-content">
                                                <span class="mooc-watch-info__item-count">117</span>
                                                <span class="mooc-watch-info__item-key">Courses<br>Added</span>
                                            </div>
                                        </div>
                                        <div class="mooc-watch-info__item">
                                            <div class="mooc-watch-info__item-content">
                                                <span class="mooc-watch-info__item-count">2893</span>
                                                <span class="mooc-watch-info__item-key">Total<br>MOOCs</span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="mooc-watch-info__news">
                                        <h3 class="mooc-watch-info__news-title">MOOC top news</h3>
                                        <div class="mooc-watch-info__news-item">
                                            <a href="" class="mooc-watch-info__news-item-title">Data Analysis and Statistical Inference</a>
                                            <span class="mooc-watch-info__news-item-provider">Coursera</span>
                                        </div>
                                        <div class="mooc-watch-info__news-item">
                                            <a href="" class="mooc-watch-info__news-item-title">How Writers Write Fiction</a>
                                            <span class="mooc-watch-info__news-item-provider">edX</span>
                                        </div>
                                        <div class="mooc-watch-info__news-item">
                                            <a href="" class="mooc-watch-info__news-item-title">Introduction to Financial Accounting</a>
                                            <span class="mooc-watch-info__news-item-provider">Udacity</span>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                    </div>


                </div>

            </div>

        </div>

    </section>

    <section id="mooc-watch-testimonials-section" class="mooc-watch-testimonials-section">

        <div class="container">

            <h2 class="mooc-watch-testimonials-section__title">What the experts say...</h2>
            <div class="testimonial-card mooc-watch-testimonials-section__testimonial-card">

                <div class="testimonial-card__author-block">
                    <div class="testimonial-card__author-content">
                        <div class="crop-circle crop-circle--bordered">
                            <img src="https://www.class-central.com/bundles/classcentralsite/slashpixel/images/profile-pic-placeholder.png" alt="profile picture">
                        </div>
                        <span class="testimonial-card__title">Prof. Benedict Cumberbatch</span>
                        <span class="testimonial-card__meta">London, England</span>
                    </div>
                </div>

                <div class="testimonial-card__testimonial-block">
                    <p class="testimonial-card__testimonial">
                        It is an executive summary of sorts for what is happening, so it serves a good purpose for college administrators and MOOC enthusiasts.
                    </p>
                </div>
            </div>

        </div>

    </section>

    <section class="footer-logos">
        <ul>
            <li><a class="stanford-logo" href="{{ path('ClassCentralSiteBundle_university',{slug : 'stanford'}) }}">Stanford University</a></li>
            <li><a class="harvard-logo" href="{{ path('ClassCentralSiteBundle_university',{slug : 'harvard'}) }}">Harvard University</a></li>
            <li><a class="mit-logo" href="{{ path('ClassCentralSiteBundle_university',{slug : 'mit'}) }}">Massachusetts Institute of Technology</a></li>
            <li><a class="berkeley-logo" href="{{ path('ClassCentralSiteBundle_university',{slug : 'berkeley'}) }}">Berkeley University of California</a></li>
            <li><a class="uts-logo" href="{{ path('ClassCentralSiteBundle_university',{slug : 'utexas'}) }}">University of Texas System</a></li>
            <li><a class="cornell-logo" href="{{ path('ClassCentralSiteBundle_university',{slug : 'cornell'}) }}">Cornell University</a></li>
            <li><a class="princeton-logo" href="{{ path('ClassCentralSiteBundle_university',{slug : 'princeton'}) }}">Princeton University</a></li>
            <li><a class="git-logo" href="{{ path('ClassCentralSiteBundle_university',{slug : 'gatech'}) }}">Georgia Tech</a></li>
            <li><a class="jhu-logo" href="{{ path('ClassCentralSiteBundle_university',{slug : 'jhu'}) }}">Johns Hopkins University</a></li>
        </ul>
    </section>

{% endblock %}