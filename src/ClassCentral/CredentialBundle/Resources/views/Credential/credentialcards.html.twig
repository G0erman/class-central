{% for result in credentials %}
    {% set credential = result._source %}
    <div class="cred-col col-xs-6 col-sm-12 col-md-6 col-lg-4">
        <div class="cred-card">
            <div class="cred-card__aspect-ratio">
                <a href="{{ path('credential_page', {slug : credential.slug} ) }}" class="cred-card__inner">
                    <div class="cred-card__overlay"></div>
                    <img class="cred-card__overlay-image" src="{{ credential.cardImage }}" alt="Credential illustration">
                    <div class="cred-card__top-content">
                        {% if credential.institutions |length > 0 %}
                            {{ credential.institutions[0].name }}
                        {% endif %}
                    </div>

                    <div class="cred-card__bookmark">
                        <span class="css-bookmark-icon css-bookmark-icon--full"></span>
                    </div>


                    <div class="cred-card__title-wrap">
                        <h3 class="cred-card__title">{{ credential.name }}</h3>
                    </div>

                    <!--
                        <span class="cred-card__flag">Sponsored</span>
                    -->

                    <div class="cred-card__footer-content">
                        <div class="cred-card__hover-content">
                            <span class="cred-card__price">{{ credential.displayPrice }}</span>
                            <span class="cred-card__provider">Via {{ credential.provider.name }}</span>
                        </div>

                        <div class="cred-card__star-rating">
                            <div class="starRating starRating-em starRating-white rating{{ credential.formattedRating*10 }}">
                                <div class="starRating_star">
                                    <svg class="starRating_starShape starRating_starShape-blank">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-full"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-full">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-full"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-left starRating_starShape-half">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-half-left"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-right starRating_starShape-half">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-half-right"></use>
                                    </svg>
                                </div>
                                <div class="starRating_star">
                                    <svg class="starRating_starShape starRating_starShape-blank">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-full"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-full">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-full"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-left starRating_starShape-half">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-half-left"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-right starRating_starShape-half">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-half-right"></use>
                                    </svg>
                                </div>
                                <div class="starRating_star">
                                    <svg class="starRating_starShape starRating_starShape-blank">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-full"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-full">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-full"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-left starRating_starShape-half">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-half-left"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-right starRating_starShape-half">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-half-right"></use>
                                    </svg>
                                </div>
                                <div class="starRating_star">
                                    <svg class="starRating_starShape starRating_starShape-blank">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-full"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-full">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-full"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-left starRating_starShape-half">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-half-left"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-right starRating_starShape-half">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-half-right"></use>
                                    </svg>
                                </div>
                                <div class="starRating_star">
                                    <svg class="starRating_starShape starRating_starShape-blank">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-full"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-full">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-full"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-left starRating_starShape-half">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-half-left"></use>
                                    </svg>
                                    <svg class="starRating_starShape starRating_starShape-right starRating_starShape-half">
                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-star-half-right"></use>
                                    </svg>
                                </div>
                            </div>

                        </div><!-- /.cred-card__star-rating -->

                    </div>
                </a><!-- /.cred-card__inner -->
            </div>

        </div>
    </div>
{% endfor %}