{% extends 'ClassCentralSiteBundle::base.html.twig' %}

{%  block headmeta %}

	<title>Reviews for {{ credential.name }} | Earn a Certificate</title>
	<meta name="description"
		  content="{{ credential.numRatings  }} Reviews for {{ credential.name }}. {{ credential.oneLiner }}"/>

	<!-- Open Graph Tags -->
	<meta property="og:type" content="website" />
	<meta property="og:title" content="Reviews for {{ credential.name }} | Earn a Certificate" />
	<meta property="og:url" content="{{ path('credential_page', {slug : credential.slug}) }}" />
	<meta property="og:description" content="{{ credential.numRatings  }} Reviews for {{ credential.name }}. {{ credential.oneLiner }}" />
	<meta property="og:image" content="{{ credential.cardImage }}" />


{% endblock %}

{% block ccfullcontent -%}
	<div class="cc-full-width-content">
		<div class="fixed-menu-spacer"></div>
		<div class="tilted-sections-wrap">

			<div class="tilted-section tilted-section--blue tilted-section--steep-bottom">
				<div class="tilted-section__content">
					<div class="container">

						<div class="single-cred-intro">

							<h1 class="single-cred-intro__title">
								{{ credential.name   }}
							</h1>
							<span class="single-cred-intro__subtitle">{{ credential.oneLiner }}</span>

							<div class="single-cred-intro__logos">

								<img class="single-cred-intro__logo" src="/bundles/classcentralsite/slashpixel/images/sample-logo-udacity.png" alt="Udacity logo">

								<span class="single-cred-intro__plus-sign">
									<span class="css-plus"></span>
								</span>

								<img class="single-cred-intro__logo" src="/bundles/classcentralsite/slashpixel/images/sample-logo-google.png" alt="Google logo">

								<span class="single-cred-intro__plus-sign">
									<span class="css-plus"></span>
								</span>

								<img class="single-cred-intro__logo" src="/bundles/classcentralsite/slashpixel/images/sample-logo-udacity.png" alt="Udacity logo">


							</div>

						</div><!-- /.single-cred-intro -->

					</div>
				</div>


			</div><!-- /.tilted-section -->

			<div class="tilted-section tilted-section--dark">
				<div class="tilted-section__tilt tilted-section__tilt--steep"></div>
				<div class="tilted-section__content">
					<div class="container">
						<div class="cred-cta-section">

							<div class="cred-cta-col cred-cta-col--left">
								<h2 class="cred-subsection-title">Earn a Certificate</h2>

								<div class="cred-subsection-list">
									<ul class="bullet-list">
										{% for bulletpoint in credential.bulletPoints %}
										    <li class="bullet-list__item">{{ bulletpoint }}</li>
										{% endfor %}
									</ul>

								</div>

							</div>
							<div class="cred-cta-col cred-cta-col--right">
								<div class="cred-cta__stamp-wrap">
									<div class="cred-cta__stamp">
										<div class="rating-stamp">
											<div class="rating-stamp__dynamic-content">
												<div class="rating-stamp__star-rating">
													{% include 'ClassCentralSiteBundle:Helpers:starRating.html.twig' with {formattedRating: credential.formattedRating,ratingColour: 'white'} %}

												</div><!-- /.rating-stamp__star-rating -->

												<div class="rating-stamp__count">{{ credential.numRatings  }} Reviews</div>
											</div>
										</div>
									</div><!-- /.cred-cta__stamp -->

									<div class="cred_cta__note">
										<span class="highlight">Rating based</span> on {{ credential.numRatings  }} student reviews. <a href="{{ path('credential_review', {credentialId: credential.id  })  }}"><span class="highlight">Write a Review</span></a>.
									</div><!-- /.cred_cta__note -->

								</div><!-- /.cred-cta__stamp-wrap -->

								<div class="cred-cta__button">
									<a href="{{ credential.url }}" class="basic-input-button">Go To Credential</a>
								</div>


							</div>
						</div><!-- /.cred-cta-section -->
